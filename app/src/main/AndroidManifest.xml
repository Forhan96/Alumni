<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.neub.alumni">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StudentsActivity" />
        <activity android:name=".FacultyActivity" />
        <activity android:name=".AlumniActivity" />
        <activity android:name=".MembersActivity" />
        <activity android:name=".StudentProfileEditActivity" />
        <activity android:name=".FacultyProfileEditActivity" />
        <activity android:name=".AlumniProfileEditActivity" />
        <activity android:name=".FacultyProfileActivity" />
        <activity android:name=".StudentProfileActivity" />
        <activity android:name=".FacultyLoginActivity" />
        <activity android:name=".StudentLoginActivity" />
        <activity android:name=".AlumniLoginActivity" />
        <activity android:name=".LoginChoiseActivity" />
        <activity android:name=".AlumniProfileActivity" />
        <activity android:name=".EventActivity" />
        <activity android:name=".NewsFeedActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LaunchingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>


</manifest>